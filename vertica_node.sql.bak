SELECT *
FROM vertex
WHERE (
REGEXP_LILE(NODE_LABEL,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(TYPE,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(FIRST_NAME,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(LAST_NAME,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(GENDER,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(EMAIL,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(SPEAKS,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(BROWSER_USED,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(LOCATION_IP,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(NAME,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(CONTENT,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(LANGUAGE,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(IMAGE_FILE,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(TITLE,'.*\bchina\b.*' ,'i') |
REGEXP_LILE(URL,'.*\bchina\b.*' ,'i') 
);